{
  "name": "More Typos Please",
  "manifest_version": 2,
  "version": "1.2",

  "description": "Reduce your typing performance by increasing the number of typos in your text.",

  "icons": {
        "16": "data/img/icon-16.png",
        "48": "data/img/icon-48.png",
        "128": "data/img/icon-128.png"
  },

  "content_scripts": [
    {
      "matches": ["http://*/*",
                  "https://*/*"],
      "js": [
        "data/js/libs/jquery-2.1.1.min.js",
        "data/js/contentscript.js"
      ],

      "run_at": "document_end"
    }
  ]
}
